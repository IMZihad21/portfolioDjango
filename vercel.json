{
  "builds": [
    {
      "src": "core/asgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "core/asgi.py"
    }
  ],
  "github": {
    "silent": true
  }
}